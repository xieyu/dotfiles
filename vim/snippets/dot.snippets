snippet subgraph 
subgraph cluster_${1:title} {
	graph[label="${1}";fontsize=20;];
	$0
}
endsnippet

snippet digraph
digraph ${1:title} {
	node[shape=box];
	edge[color=gray40];
	newrank=true;
	rankdir=LR;
	labelloc=t
	label=<<B>$1</B>>
	${0:a->b}
}
endsnippet

snippet sf "style=filled"
style="filled"
endsnippet

snippet htmlTable "HTML like label: table"
${1:html}[
	shape=none,
	border=none,
	label=<<table title="$1" border="0" cellborder="1" cellspacing="0" cellpadding="1">
		<tr><td bgcolor="silver"><b>$1</b></td></tr>
	</table>>];
endsnippet

snippet trtd "tr td"
<tr><td>$1</td></tr>
endsnippet

snippet trtd_gray "tr with gray font color"
<tr><td><font color="gray">
	$1
</font></td></tr>
endsnippet

snippet tr "tr"
<tr>$1</tr>
endsnippet
	
	

snippet tr1td "tr in table"
<tr>
	<td colspan="4">$1</td> 
</tr>
endsnippet

snippet td "td in tr"
