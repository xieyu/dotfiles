snippet tr3td "tr in table"
<tr>
	<td colspan="4">$1</td> 
	<td colspan="4">$2</td> 
	<td colspan="4">$3</td> 
</tr>
endsnippet

snippet img "img in table"
<td><IMG SRC="$1"/></td>
endsnippet
	

snippet record
${1:record}[
	shape="record";
	label="{{
		${1}|
			${2:field} \l
	}}";
];
endsnippet

snippet redrecord
${1:record}[
	shape="record";
	fillcolor="#f38181";
	style=filled;
	label="{{
		${1}|
			${2:field} \l
	}}";
];
endsnippet

snippet tearecord
